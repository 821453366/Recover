<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eu.front.dao.StockDao">
    <select id="queryStock" resultType="map">
        SELECT * FROM tb_stock where stock_code like #{stockCode} order by id DESC  LIMIT #{start}, #{end}
    </select>
    <select id="queryStockCount" resultType="long">
        SELECT count(1)
        FROM tb_stock
    </select>
</mapper>